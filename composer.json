{
	"name": "orisai/reflection-meta",
	"description": "PHP reflection in more reliable and deterministic way - for declarative engines",
	"keywords": [
		"annotations",
		"attributes",
		"classes",
		"constants",
		"interfaces",
		"meta",
		"methods",
		"orisai",
		"properties",
		"reflection",
		"traits"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/reflection-meta",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.3.0",
		"orisai/exceptions": "^1.0.0",
		"orisai/source-map": "^1.0.0",
		"orisai/utils": "^1.0.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"doctrine/annotations": "^1.12.1|^2.0.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^3.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0"
	},
	"autoload": {
		"psr-4": {
			"Orisai\\ReflectionMeta\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Orisai\\ReflectionMeta\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
